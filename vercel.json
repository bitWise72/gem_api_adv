{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/get_recipe",
      "methods": ["POST","OPTIONS"],
      "dest": "/api/index.py"
    },
    {
      "src": "/get_nutri",
      "methods": ["POST","OPTIONS"],
      "dest": "/api/index.py"
    },
    {
      "src": "/test",
      "methods": ["GET"],
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "/api/index.py"
    }
  ]
}
